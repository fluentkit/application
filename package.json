        "vue": "^2.6.11",
        "vue-router": "^3.1.5",
        "vue-template-compiler": "^2.6.11",
        "vue-toasted": "^1.1.27",
        "vuex": "^3.3.0",
        "vuex-router-sync": "^5.0.0"
    }
